__entryPoint_                     LDI                          0x80
                                  STW                          romUser
                                  RomCheck                     
                                  Initialise                                                ; INIT

                                  MOVQW                        graphicsMode, 3
                                  ScanlineMode                                              ; mode 3

                                  ClearVertBlinds                                           ; cls

                                  MOVQW                        fgbgColour, 0                ; set FGBG_COLOUR, &h0000

                                  LDNI                         60
                                  STW                          _cy                          ; cy = ymin

                                  MOVQW                        giga_secondsTickLo, 0
                                  MOVQB                        giga_jiffiesTick, 0          ; set TIMER, 0

                                  MOVQW                        _py, 0                       ; for py=0 &to 59

_next_0x0000                      LDNI                         90
                                  STW                          _cx                          ; cx = xmin

                                  LDI                          119
                                  SUBW                         _py
                                  STW                          _ipy                         ; ipy = 119 - py

                                  MOVQW                        _px, 0                       ; for px=0 &to 159

_next_0x0001                      LDI                          0
                                  STW                          _x
                                  STW                          _y
                                  STW                          _x2
                                  STW                          _y2                          ; x=0 : y=x : x2=y : y2=x2

                                  MOVQW                        _c, 15                       ; for c=15 &downto 0

_next_0x0002                      LDW                          _x
                                  ABSW                         
                                  SUBI                         64
                                  JLE                          _if_0x0003
                                  BRA                          _break                       ; if abs(x) > (2**fracbits * 2) then goto &break

_if_0x0003                        LDW                          _y
                                  ABSW                         
                                  SUBI                         64
                                  JLE                          _if_0x0004
                                  BRA                          _break                       ; if abs(y) > (2**fracbits * 2) then goto &break

_if_0x0004                        MOVWA                        _x, giga_sysArg0
                                  STW                          giga_sysArg2
                                  CALLI                        multiply16bit
                                  STW                          mathShift
                                  Lsr5bit                      giga_vAC
                                  STW                          _x2                          ; x2 = (x*x) LSR fracbits

                                  MOVWA                        _y, giga_sysArg0
                                  STW                          giga_sysArg2
                                  CALLI                        multiply16bit
                                  STW                          mathShift
                                  Lsr5bit                      giga_vAC
                                  STW                          _y2                          ; y2 = (y*y) LSR fracbits

                                  LDW                          _x2
                                  ADDW                         _y2
                                  SUBI                         128
                                  JLE                          _if_0x0005
                                  BRA                          _break                       ; if x2+y2 > (2**fracbits * 4) then goto &break

_if_0x0005                        LDW                          _x2
                                  SUBW                         _y2
                                  ADDW                         _cx
                                  STW                          _xt                          ; xt = x2 - y2 + cx

                                  MOVWA                        _x, giga_sysArg0
                                  MOVWA                        _y, giga_sysArg2
                                  CALLI                        multiply16bit
                                  STW                          mathShift
                                  Asr4bit                      
                                  ADDW                         _cy
                                  STW                          _yt                          ; yt = ((x*y) ASR (fracbits-1)) + cy

                                  LDW                          _y
                                  XORW                         _yt
                                  JNE                          _endif_0x0006                ; if y = yt

                                  LDW                          _x
                                  XORW                         _xt
                                  JNE                          _endif_0x0007                ; if x = xt

                                  BRA                          _done                        ; goto &done

_endif_0x0006                     MOVWA                        _yt, _y                      ; y = yt

                                  MOVWA                        _xt, _x                      ; x = xt

                                  ForNextDBGE                  _c _next_0x0002              ; next c

_done                             MOVQW                        _c, 0                        ; c = 0

_break                            LDARRB                       _c, 1536
                                  STW                          _c                           ; c = peek(colours + c)

                                  PACKVW                       _px, _py
                                  STPX                         _c                           ; pset px, py, c

                                  PACKVW                       _px, _ipy
                                  STPX                         _c                           ; pset px, ipy, c

                                  ADDVI                        _cx, 1                       ; cx = cx + dx

                                  ForNextInc                   _px _next_0x0001 159         ; next px

                                  ADDVI                        _cy, 1                       ; cy = cy + dy

                                  ForNextInc                   _py _next_0x0000 59          ; next py

                                  LD                           giga_jiffiesTick
                                  STW                          _jiffies                     ; jiffies = get("JIFFIES")

                                  MOVWA                        giga_secondsTickLo, _seconds ; seconds = get("TIMER")

                                  CALLI                        _page_0x0300
_page_0x0300                      LDWI                         16128
                                  STW                          fgbgColour                   ; set FGBG_COLOUR, &h3F00

                                  MOVQB                        cursorXY, 50
                                  MOVQB                        cursorXY + 1, 56
                                  AtTextCursor                 
                                  LDW                          _seconds
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7dfd
                                  LDW                          _jiffies
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7df8
                                  NewLine                                                   ; at 50, 56 : print seconds;" ";jiffies;"/60"

                                  HALT                                                      ; END



