                                  MOVQW                        drawCircleF_cx, 80
                                  MOVQW                        drawCircleF_cy, 60
                                  MOVQW                        drawCircleF_r, 60
                                  DrawCircleF                                               ; circlef 80, 60, 60

                                  ForNextInc                   _i _next_0x0002 100          ; next i

                                  MOVWA                        giga_secondsTickLo, _timerCS ; timerCS = get("TIMER")

                                  LD                           giga_jiffiesTick
                                  STW                          _timerCJ                     ; timerCJ = get("JIFFIES")

                                  ClearVertBlinds                                           ; cls

                                  MOVQB                        fgbgColour + 1, 63           ; set FG_COLOUR, &h3F

                                  CALLI                        _page_0x0300
_page_0x0300                      PrintString                  _str_0x7df7
                                  LDW                          _timerLS
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7df4
                                  LDW                          _timerLJ
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7def
                                  NewLine                                                   ; print "Lines: ";timerLS;" ";timerLJ;"/60"

                                  PrintString                  _str_0x7de6
                                  LDW                          _timerRS
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7df4
                                  LDW                          _timerRJ
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7def
                                  NewLine                                                   ; print "Rects: ";timerRS;" ";timerRJ;"/60"

                                  PrintString                  _str_0x7ddd
                                  LDW                          _timerCS
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7df4
                                  LDW                          _timerCJ
                                  PrintAcInt16                 
                                  PrintString                  _str_0x7def
                                  NewLine                                                   ; print "Circs: ";timerCS;" ";timerCJ;"/60"

                                  HALT                                                      ; END



