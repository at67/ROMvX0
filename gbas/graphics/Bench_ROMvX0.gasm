                                  MOVQW                        drawLine_y1, 0
                                  MOVQW                        drawLine_x2, 159
                                  MOVQW                        drawLine_y2, 119
                                  DrawLine                                                  ; line  0,   0,   159, 119

                                  MOVQW                        drawLine_x1, 0
                                  MOVQW                        drawLine_y1, 119
                                  MOVQW                        drawLine_x2, 159
                                  MOVQW                        drawLine_y2, 0
                                  DrawLine                                                  ; line  0,   119, 159, 0

                                  ForNextInc                   _i _next_0x0000 100          ; next i

                                  MOVWA                        giga_secondsTickLo, _timerLS ; timerLS = get("TIMER")

                                  LD                           giga_jiffiesTick
                                  STW                          _timerLJ                     ; timerLJ = get("JIFFIES")

                                  ClearVertBlinds                                           ; cls

                                  MOVQW                        giga_secondsTickLo, 0
                                  MOVQB                        giga_jiffiesTick, 0          ; set TIMER, 0

                                  MOVQW                        _i, 1                        ; for i=1 to 100

_next_0x0001                      Rand                         
                                  ANDI                         0x3f
                                  ST                           fgbgColour + 1               ; set FG_COLOUR, rnd(0) AND &h3F

                                  MOVQW                        drawRectF_x1, 0
                                  MOVQW                        drawRectF_y1, 0
                                  MOVQW                        drawRectF_x2, 159
                                  MOVQW                        drawRectF_y2, 119
                                  DrawRectF                                                 ; rectf 0, 0, 159, 119

                                  ForNextInc                   _i _next_0x0001 100          ; next i

                                  MOVWA                        giga_secondsTickLo, _timerRS ; timerRS = get("TIMER")

                                  LD                           giga_jiffiesTick
                                  STW                          _timerRJ                     ; timerRJ = get("JIFFIES")

                                  ClearVertBlinds                                           ; cls

                                  MOVQW                        giga_secondsTickLo, 0
                                  MOVQB                        giga_jiffiesTick, 0          ; set TIMER, 0

                                  MOVQW                        _i, 1                        ; for i=1 to 100

_next_0x0002                      Rand                         
                                  ANDI                         0x3f
